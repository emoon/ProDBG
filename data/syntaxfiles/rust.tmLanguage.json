                    }
                },
                {
                    "match": "([a-zA-Z_]+)(?=[:] )",
                    "captures": {
                        "1": {
                            "name": "variable.other.rust"
                        }
                    }
                },
                {
                    "match": "(let|pub)\\s([a-zA-Z0-9_]+)(\\:\\s)([a-zA-Z0-9_]+)",
                    "captures": {
                        "1": {
                            "name": "storage.type.rust"
                        },
                        "2": {
                            "name": "variable.other.rust"
                        },
                        "3": {
                            "name": "punctuation.separator.type.rust"
                        },
                        "4": {
